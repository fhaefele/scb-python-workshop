# help(str.lower)
# help(str.upper)

def jitter_string(my_string):

    new_string = ''
    for ix, s in enumerate(my_string):
        if ix % 2 == 0:
            new_string += s.lower()
        else:
            new_string += s.upper()

    return new_string

# Check
jitter_string('Anthropomorphism')





