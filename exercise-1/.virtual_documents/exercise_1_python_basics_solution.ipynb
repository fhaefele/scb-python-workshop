def master_yoda(text):
    return ' '.join(text.split(' ')[::-1])

# Check
print(master_yoda('I am home'))
print(master_yoda('We are ready'))





def master_yoda(text):
    sp = text.split(' ')[::-1]
    sp[0] = sp[0].capitalize()
    sp[-1] = sp[-1].lower()
    return ' '.join(sp)

# Check
print(master_yoda('I am home'))
print(master_yoda('We are ready'))





